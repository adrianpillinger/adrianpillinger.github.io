---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Pillinger.co</title>
 <link href="http://www.pillinger.co/atom.xml" rel="self"/>
 <link href="http://www.pillinger.co/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.pillinger.co/</id>
 <author>
   <name>Adrian Pillinger</name>
<!--   <email></email>-->
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.pillinger.co{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.pillinger.co{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>